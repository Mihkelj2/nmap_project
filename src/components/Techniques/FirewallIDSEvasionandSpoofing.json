  },
  {
    "Switch": "-g",
    "Example": "nmap -g 53 192.168.1.1",
    "Description": "Use given source port number"
  },
  {
    "Switch": "--proxies",
    "Example": "nmap --proxies http://192.168.1.1:8080, http://192.168.1.2:8080 192.168.1.1",
